num = int(input(" Enter how many numbers u want to see : "))

for i in range(num):
  if i % 2 == 0:
    print("square of %s is %s"%(i,i**2))
  else:
    print("cube of %s is %s"%(i,i**3))


===============LIST COMPREHENSION==================


num = int(input(" Enter how many numbers u want to see : "))


out = [i**2 if i%2 ==0 else i**3 for i in range(num)]

print(out)
